{
  "name": "Blank New Tab",
  "version": "0.1",
  "description": "An open-source, blank new tab page.",
  "manifest_version": 2,
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "options_page": "options.html",
  "options_ui": {
    "page": "options.html",
    "chrome_style": false,
    "open_in_tab": true
  },
  "permissions": [
    "storage"
  ]
}